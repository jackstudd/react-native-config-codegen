{
  "name": "react-native-config-codegen",
  "version": "1.0.0",
  "description": "Quickly generate a TypeScript declaration file for your .env files",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "jackstudd",
  "license": "MIT",
  "homepage": "https://github.com/jackstudd/react-native-config-codegen",
  "repository": {
    "type": "git",
    "url": "https://github.com/jackstudd/react-native-config-codegen"
  },
  "keywords": [
    "react-native-config",
    "declaration",
    "types",
    "typescript",
    "env",
    "config",
    "config-var",
    "react-native"
  ],
  "files": [
    "lib/**/*"
  ],
  "bin": "lib/index.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "yarn build"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/node": "^18.14.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "prettier": {
    "bracketSameLine": true,
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
